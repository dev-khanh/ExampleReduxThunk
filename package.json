{
  "name": "ExampleReduxThunk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "bundle:ios": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios' --assets-dest='./ios' --sourcemap-output sourcemap.ios.js.map --sourcemap-sources-root ./",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/index.android.bundle --sourcemap-output sourcemap.android.js.map --sourcemap-sources-root ./",
    "pod": "cd ios && pod install && cd ..",
    "xcode": "cd ios && open ExampleReduxThunk.xcworkspace && cd ..",
    "studio": "open -a /Applications/AndroidStudio.app/Contents/MacOS/studio android",
    "android-replase": "cd android && ./gradlew clean && ./gradlew build && cd .."
  },
  "dependencies": {
    "@heap/react-native-heap": "^0.14.0",
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.10",
    "@react-navigation/native": "^5.9.2",
    "@react-navigation/stack": "^5.14.2",
    "@reduxjs/toolkit": "^1.5.0",
    "axios": "^0.21.1",
    "form-data": "^3.0.0",
    "lodash": "^4.17.20",
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-dotenv": "^2.5.1",
    "react-native-gesture-handler": "^1.9.0",
    "react-native-reanimated": "^1.13.2",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-screens": "^2.17.1",
    "react-redux": "^7.2.2",
    "reactotron-redux": "^3.1.3",
    "redux": "^4.0.5",
    "redux-persist": "^6.0.0",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/runtime": "^7.8.4",
    "@react-native-community/eslint-config": "^1.1.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^25.1.0",
    "babel-plugin-module-resolver": "^4.1.0",
    "eslint": "^6.5.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-babel-module": "^5.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-native": "^3.10.0",
    "jest": "^25.1.0",
    "jetifier": "^1.6.6",
    "metro-react-native-babel-preset": "^0.59.0",
    "react-native-rename": "^2.7.0",
    "react-test-renderer": "^17.0.1",
    "reactotron-react-native": "^5.0.0"
  },
  "jest": {
    "preset": "react-native"
  }
}
